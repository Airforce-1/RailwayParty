                android:layout_weight="1" />

            <TextView
                android:id="@+id/text_total_pay"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="2"
                android:gravity="center"
                android:text="共计0元"
                android:textSize="@dimen/member_transform_size" />

