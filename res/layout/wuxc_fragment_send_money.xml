<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#f5f5f5"
    android:orientation="vertical" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="90dp"
        android:gravity="center"
        android:orientation="horizontal" >

        <wuxc.single.railwayparty.layout.RoundImageView
            android:id="@+id/image_headimg"
            android:layout_width="70dp"
            android:layout_height="70dp"
            android:src="@drawable/t0145ec17ae33bd0b7b" />

        <TextView
            android:layout_width="3dp"
            android:layout_height="40dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:background="#000000" />

        <TextView
            android:id="@+id/text_name"
            android:layout_width="70dp"
            android:layout_height="30dp"
            android:gravity="center_vertical"
            android:singleLine="true"
            android:text="李煜新"
            android:textSize="16dp" />
    </LinearLayout>

    <TextView
        android:id="@+id/text_last_month"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_marginLeft="50dp"
        android:gravity="center_vertical"
        android:singleLine="true"
        android:text="最后缴纳月份：2017年2月"
        android:textSize="16dp" />

    <TextView
        android:id="@+id/text_last_time"
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:layout_marginLeft="50dp"
        android:gravity="center_vertical"
        android:singleLine="true"
        android:text="缴费时间：2017年02月28号"
        android:textSize="16dp" />

    <Button
        android:id="@+id/btn_ok"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_marginLeft="25dp"
        android:layout_marginRight="25dp"
        android:background="@drawable/shape7red"
        android:gravity="center"
        android:text="申请缴费"
        android:textColor="#ffffff"
        android:textSize="16dp" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginLeft="25dp"
        android:layout_marginRight="25dp"
        android:orientation="vertical" >

        <RelativeLayout
            android:id="@+id/relative_result"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="2" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="center"
                android:orientation="horizontal" >

                <ImageView
                    android:id="@+id/image_left"
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    android:paddingLeft="10dp"
                    android:paddingRight="10dp"
                    android:src="@drawable/arrow_grey_left" />

                <TextView
                    android:id="@+id/text_year"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:gravity="center"
                    android:text="2016"
                    android:textColor="@color/member_transform_party_top_red"
                    android:textSize="@dimen/member_transform_size" />

                <ImageView
                    android:id="@+id/image_right"
                    android:layout_width="wrap_content"
                    android:layout_height="35dp"
                    android:paddingLeft="10dp"
                    android:src="@drawable/arrow_grey_right" />
            </LinearLayout>
        </RelativeLayout>

        <LinearLayout
            android:id="@+id/lin_result"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/shape_7_white"
            android:orientation="vertical" >

